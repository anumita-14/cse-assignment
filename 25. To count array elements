#include <stdio.h>
#include<conio.h>
int main() {
    int i,n,count=0;
    printf("Enter value of n: \n");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        count++;
    }
    printf("No. of elements present in array : %d",count);
    getch();
    return 0;
}
