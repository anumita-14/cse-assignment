#include<stdio.h>
#include<conio.h>
int main()
{
    int i,n,g;
    printf("Enter the value of n:\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter %d elements \n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    g=a[0];
    for(i=1;i<n;i++)
    {
        if(a[i]>g)
        g=a[i];
    }
    printf(" Greatest no. is %d",g);
    getch();
    return 0;
}
