#include<stdio.h>
#include<conio.h>
int main()
{
    int yr;
    printf("Enter a year to check if it's a leap year or not: ");
    scanf("%d",&yr);
    if(yr%4==0)
    {
    if(yr%100==0)
    {
        if(yr%400==0)
        {
         printf("It is a leap year");
            }
            else
             printf("It is not a leap year");
    }
            else
                printf("It is a leap year");
}
else
    printf("It is not a leap year");
    getch();
    return(0);
}
