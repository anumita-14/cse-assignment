#include<stdio.h>
#include<conio.h>
int main()
{
    int i,n,sum=0;
    printf("Enter the value of n:\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter %d elements \n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        sum+=a[i];
    }
    printf("Sum of all elements is %d",sum);
    getch();
    return 0;
}
