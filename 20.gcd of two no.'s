#include<stdio.h>
#incude<conio.h>
int main()
{
    int i,a,b,gcd;
    printf("Enter two numbers:\n");
    scanf("%d %d",&a,&b);
    for(i=1;i<=a && i<=b;i++)
    {
        if(a%i==0 && b%i==0)
        gcd=i;
    }
        printf("gcd of %d and %d is: %d",a,b,gcd);
        getch();
        return 0;
}
