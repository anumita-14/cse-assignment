#include<stdio.h>
#include<string.h>
 
int main ()
{
    char s[100],s1[100];
    int t=0,i,j;
    printf("enter the string:");
    gets(s);
    for (i=0,j=0;s[i]!='\0';i++)
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u');
        else
        {
          s1[j++]=s[i];
        }
    }
    printf("string after removals of vowels: %s",s1);
   return 0; 
}
