#include<stdio.h>
#include<conio.h>
int main()
{
    int a,b,c;
    printf("Enter 3 no.'s: \n");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b && a>c)
    printf("Greatest no. is %d",a);
    else if(b>a &&b>c)
    printf("Greatest no. is %d",b);
    else if(c>a && c>b)
    printf("Greatest no. is %d",c);
    else
    printf("All 3 no.'s are equal");
    getch();
    return 0;
}
