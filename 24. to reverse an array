#include <stdio.h>
#include<conio.h>
int main() {
    int i,n;
    printf("Enter the value of n \n");
    scanf("%d",&n);
    int a[n];
    printf("Enter %d elements: \n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("On Revesing the array \n");
    for(i=n-1;i>=0;i--)
    {
      printf("%d ",a[i]);  
    }
    getch();
    return 0;
}
